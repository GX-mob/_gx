{
  "expo": {
    "name": "voyager",
    "slug": "voyager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "entryPoint": "./src/Launcher.tsx",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "userInterfaceStyle": "dark",
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
