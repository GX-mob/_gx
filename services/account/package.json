{
  "name": "gx-account-service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/index.js",
    "test": "NODE_ENV=test jest --runInBand",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "rimraf ./dist && tsc --sourceMap -p ./ && copyfiles -u 1 \"./src/**/*.css\" ./dist",
