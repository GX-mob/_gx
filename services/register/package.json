    "lint-staged": "^10.2.11",
    "mongodb-memory-server": "^6.6.3",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
