    "fastify-plugin": "^2.0.3",
    "http-errors": "^1.8.0",
    "ioredis": "^4.17.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "source-map-support": "^0.5.19"
  },
