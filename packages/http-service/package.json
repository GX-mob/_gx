{
  "name": "@gx-mob/http-service",
  "version": "0.0.38",
  "description": "Base modules of http services",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/GX-mob/http-service-template.git"
  },
  "scripts": {
    "start": "node ./dist/bin/index.js",
    "test": "NODE_ENV=test jest --runInBand --forceExit --detectOpenHandles",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "rimraf ./dist && tsc --sourceMap -p ./",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' --files ./src/bin/index.ts",
    "compile-schemas": "json2ts -i ./src/schemas -o ./src/types"
  },
  "author": "ferco0",
  "license": "AGPL-3.0",
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.10",
    "@google-cloud/storage": "^5.1.2",
    "fastify": "^3.2.1",
    "fastify-circuit-breaker": "^1.0.1",
    "fastify-decorators": "^3.1.1",
    "fastify-multipart": "^2.0.3",
    "fastify-plugin": "^2.3.2",
    "fastify-rate-limit": "^4.0.2",
    "fastify-redis": "^4.0.3",
    "fastify-swagger": "^3.3.0",
    "file-type": "^14.7.0",
    "http-errors": "^1.8.0",
    "ioredis": "^4.17.3",
    "jpegtran": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0",
    "pino": "^6.5.1",
    "pngquant": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "schemapack": "^1.4.2",
    "secure-password": "^4.0.0",
    "shortid": "^2.2.15",
    "source-map-support": "^0.5.19",
    "stream-file-type": "^0.5.0",
    "twilio": "^3.49.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.1",
    "jest-runner-groups": "^2.0.1",
    "mongodb-memory-server": "^6.6.4",
    "stream-buffers": "^3.0.2",
    "ts-jest": "^26.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint --fix"
    ]
  }
}
